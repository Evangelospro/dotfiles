{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "palette": {
        "black": "#282A36",
        "cyan": "#8BE9FD",
        "green": "#50FA7B",
        "orange": "#ffb86c",
        "red": "#FF5555",
        "white": "#ffffff",
        "yellow": "#F1FA8C",
        "purple": "#BD93F9",
        "pink": "#FF79C6"
    },
    "secondary_prompt": {
        "foreground_templates": [
            "{{if gt .Code 0}}p:red{{end}}",
            "{{if eq .Code 0}}p:purple{{end}}"
        ],
        "background": "transparent",
        "template": "❯❯"
    },
    "transient_prompt": {
        "foreground_templates": [
            "{{if gt .Code 0}}p:red{{end}}",
            "{{if eq .Code 0}}p:purple{{end}}"
        ],
        "background": "transparent",
        "template": "❯ "
    },
    "console_title_template": "{{ .Shell }} in {{ .Folder }}",
    "upgrade": {
        "source": "cdn",
        "interval": "168h",
        "auto": false,
        "notice": false
    },
    "blocks": [
        {
            "type": "prompt",
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "type": "os",
                    "style": "powerline",
                    "foreground": "p:cyan",
                    "background": "p:white",
                    "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} "
                },
                {
                    "properties": {
                        "style": "folder"
                    },
                    "template": " \uea83 {{ .PWD }} ",
                    "foreground": "p:black",
                    "powerline_symbol": "\ue0b0",
                    "background": "p:purple",
                    "type": "path",
                    "style": "powerline"
                }
            ]
        },
        {
            "type": "rprompt",
            "segments": [
                {
                    "properties": {
                        "display_mode": "files",
                        "fetch_package_manager": false,
                        "home_enabled": false
                    },
                    "template": "\ue718 ",
                    "foreground": "p:green",
                    "background": "transparent",
                    "type": "node",
                    "style": "plain"
                },
                {
                    "properties": {
                        "fetch_version": false
                    },
                    "template": "\ue626 ",
                    "foreground": "p:cyan",
                    "background": "transparent",
                    "type": "go",
                    "style": "plain"
                },
                {
                    "properties": {
                        "display_mode": "files",
                        "fetch_version": false,
                        "fetch_virtual_env": false
                    },
                    "template": "\ue235 ",
                    "foreground": "p:yellow",
                    "background": "transparent",
                    "type": "python",
                    "style": "plain"
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "type": "text",
                    "style": "plain",
                    "foreground_templates": [
                        "{{if gt .Code 0}}p:red{{end}}",
                        "{{if eq .Code 0}}p:purple{{end}}"
                    ],
                    "background": "transparent",
                    "template": "❯"
                }
            ]
        }
    ],
    "tooltips": [
        {
            "properties": {
                "display_default": true
            },
            "leading_diamond": "\ue0b0",
            "trailing_diamond": "\ue0b4",
            "template": " \ue7ad {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} ",
            "foreground": "p:white",
            "background": "p:orange",
            "type": "aws",
            "style": "diamond",
            "tips": [
                "aws"
            ]
        },
        {
            "properties": {
                "display_default": true
            },
            "leading_diamond": "\ue0b0",
            "trailing_diamond": "\ue0b4",
            "template": " \uebd8 {{ .Name }} ",
            "foreground": "p:white",
            "background": "p:purple",
            "type": "az",
            "style": "diamond",
            "tips": [
                "az"
            ]
        }
    ],
    "version": 3,
    "final_space": true
}
