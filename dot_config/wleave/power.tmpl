{
    "label": "lock",
    "action": "{{.utilities.lockscreen.linux.command }}",
    "text": "Lock",
    "keybind": "l"
}
{
    "label": "reboot",
    "action": "reboot-menu",
    "text": "Reboot",
    "keybind": "r"
}
{
    "label": "shutdown",
    "action": "systemctl poweroff",
    "text": "Shutdown",
    "keybind": "s"
}
{
    "label": "logout",
    "action": "/usr/bin/loginctl terminate-user ''",
    "text": "Logout",
    "keybind": "e"
}
{
    "label": "suspend",
    "action": "systemctl suspend",
    "text": "Suspend",
    "keybind": "u"
}
