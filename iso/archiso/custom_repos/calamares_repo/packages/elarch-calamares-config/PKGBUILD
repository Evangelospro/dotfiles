pkgname=elarch-calamares-config
_buildDir=$(pwd)
_destname1="/etc"
pkgver=1.0
pkgrel=1
pkgdesc="Calamares config for ELARCH"
arch=('any')
url=""
license=('GPL3')
provides=("${pkgname}")
options=(!strip !emptydirs)
depends=('calamares')
source=()
sha256sums=()

package() {
	# install -dm755 ${pkgdir}${_destname1}
	cp -r ${_buildDir}/etc/calamares ${pkgdir}${_destname1}
}
